    "tokenColors": [
        {
            "name": "comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#586e75"
            }
        },
        {
            "name": "keywords",
            "scope": [
                "keyword.control",
                "storage.type",
                "storage.modifier.java",
                "punctuation.terminator",
                "variable.language.this",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "strings",
            "scope": [
                "string",
                "string.regexp",
                "constant.other.symbol",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#2aa198"
            }
        },
        {
            "name": "string escape sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#6c71c4"
            }
        },
        {
            "name": "numbers and characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.keyword",
                "constant",
                "support.constant"
            ],
            "settings": {
                "foreground": "#d33682"
            }
        },
        {
            "name": "functions",
            "scope": "entity.name.function,support.function",
            "settings": {
                "foreground": "#b58900"
            }
        },
        {
            "name": "attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#b58900"
            }
        },
        {
            "name": "global definitions and properties",
            "scope": [
                "entity.name",
                "support.type.property-name.json",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#6c71c4"
            }
        },
        {
            "name": "punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#839496"
            }
        },
        {
            "name": "annotations",
            "scope": "storage.type.annotation",
            "settings": {
                "foreground": "#268bd2"
            }
        },
        {
            "name": "remove highlights",
            "scope": [
                "storage.type.java",
                "storage.type.generic",
                "keyword.control.ternary"
            ],
            "settings": {
                "foreground": "#839496"
            }
        }
    ]
}
